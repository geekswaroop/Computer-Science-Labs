#! /usr/local/bin/vvp
:ivl_version "11.0 (devel)" "(s20150603-664-g65aff653)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "vhdl_textio";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x555dd109e410 .scope module, "hello" "hello" 2 1;
 .timescale 0 0;
    .scope S_0x555dd109e410;
T_0 ;
    %vpi_func 2 4 "$time" 64 {0 0 0};
    %cmpi/u 100, 0, 64;
    %flag_or 5, 4;
    %jmp/0xz  T_0.0, 5;
    %vpi_call 2 5 "$display", "Hello World!\011%d", $time {0 0 0};
    %delay 10, 0;
    %jmp T_0.1;
T_0.0 ;
    %vpi_call 2 8 "$finish" {0 0 0};
T_0.1 ;
    %jmp T_0;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "hello.v";
